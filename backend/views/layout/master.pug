    link(href='/backend/main.css' rel='stylesheet')
    link(href='/backend/custom.css' rel='stylesheet')
    block css
.app-container.app-theme-white.body-tabs-shadow.fixed-sidebar.fixed-header
    include header
    include setting
    .app-main
        .app-sidebar.sidebar-shadow
            .app-header__logo
                .logo-src
                .header__pane.ml-auto
                    div
                        button.hamburger.close-sidebar-btn.hamburger--elastic(type='button' data-class='closed-sidebar')
                            span.hamburger-box
                                span.hamburger-inner
            .app-header__mobile-menu
                div
                    button.hamburger.hamburger--elastic.mobile-toggle-nav(type='button')
                        span.hamburger-box
                            span.hamburger-inner
            .app-header__menu
                span
                    button.btn-icon.btn-icon-only.btn.btn-primary.btn-sm.mobile-toggle-header-nav(type='button')
                        span.btn-icon-wrapper
                            i.fa.fa-ellipsis-v.fa-w-6
            include sidebar
        .app-main__outer
            .app-main__inner
                block content
            include footer
        script(src='http://maps.google.com/maps/api/js?sensor=true')
script(type='text/javascript' src='/jquery.min.js')
script(type='text/javascript' src='/backend/assets/scripts/main.js')
block js
include filemanager